   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([10, 12, 14, 16, 18])\n",
    "y = np.array([100, 144, 196, 256, 324])\n",
    "\n",
    "n = len(x)\n",
    "h = x[1] - x[0]  # Step size\n",
    "\n",
    "# Find the base point index (x0 = 12)\n",
    "x_interp = 13\n",
    "index = np.where(x <= x_interp)[0][-1]  # Index of x0=12\n",
    "x0, y0 = x[index], y[index]\n",
    "\n",
    "s = (x_interp - x0) / h\n",
    "\n",
    "# Compute first and second differences\n",
    "delta_y = np.diff(y)\n",
    "delta2_y = np.diff(delta_y)\n",
    "\n",
    "# Extract required differences\n",
    "delta_f0 = delta_y[index]      # Δf0 = 52 (forward difference at x0=12)\n",
    "delta_f_1 = delta_y[index-1]   # Δf-1 = 44 (backward difference at x0=12)\n",
    "delta2_f_1 = delta2_y[index-1] # Δ²f-1 = 8 (second difference)\n",
    "\n",
    "# Stirling's formula (up to second difference)\n",
    "estimate = y0 + s * (delta_f0 + delta_f_1) / 2 + (s**2) * delta2_f_1 / 2\n",
    "\n",
    "print(f\"Estimated f(13) = {estimate:.1f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(min(x), max(x), 100)\n",
    "y_vals = x_vals**2  # Exact function y = x²\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Exact function $y = x^2$')\n",
    "plt.plot(x_interp, estimate, 'go', label='Interpolated point (13)')\n",
    "plt.title(\"Central Difference Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x)')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
