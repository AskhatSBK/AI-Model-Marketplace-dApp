   ],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([0, 2, 5, 8])\n",
    "y = np.array([4, 8, 14, 25])\n",
    "\n",
    "# Compute divided differences\n",
    "def divided_diff(x, y):\n",
    "    n = len(y)\n",
    "    coef = np.zeros([n, n])\n",
    "    coef[:,0] = y\n",
    "    for j in range(1, n):\n",
    "        for i in range(n - j):\n",
    "            coef[i][j] = (coef[i+1][j-1] - coef[i][j-1]) / (x[i+j] - x[i])\n",
    "    return coef\n",
    "\n",
    "coeff = divided_diff(x, y)[0]\n",
    "\n",
    "# Newton's divided difference formula\n",
    "def newton_interp(x_val, x_data, coeff):\n",
    "    n = len(coeff)\n",
    "    result = coeff[0]\n",
    "    product_term = 1\n",
    "    for i in range(1, n):\n",
    "        product_term *= (x_val - x_data[i-1])\n",
    "        result += coeff[i] * product_term\n",
    "    return result\n",
    "\n",
    "x_interp = 3\n",
    "y_interp = newton_interp(x_interp, x, coeff)\n",
    "\n",
    "print(f\"Estimated f(3) = {y_interp:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(min(x), max(x), 100)\n",
    "y_vals = [newton_interp(xi, x, coeff) for xi in x_vals]\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Interpolating polynomial')\n",
    "plt.plot(x_interp, y_interp, 'go', label='Interpolated point (3)')\n",
    "plt.title(\"Newton's Divided Difference Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x)')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
