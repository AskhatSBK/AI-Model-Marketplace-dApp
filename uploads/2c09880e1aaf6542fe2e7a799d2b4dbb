   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([2, 5, 8, 10])\n",
    "y = np.array([1.4, 2.3, 3.8, 4.6])\n",
    "\n",
    "# Lagrange interpolation function\n",
    "def lagrange_interpolation(x_val, x_data, y_data):\n",
    "    n = len(x_data)\n",
    "    result = 0.0\n",
    "    for i in range(n):\n",
    "        term = y_data[i]\n",
    "        for j in range(n):\n",
    "            if j != i:\n",
    "                term *= (x_val - x_data[j]) / (x_data[i] - x_data[j])\n",
    "        result += term\n",
    "    return result\n",
    "\n",
    "x_interp = 6\n",
    "y_interp = lagrange_interpolation(x_interp, x, y)\n",
    "\n",
    "print(f\"Estimated f(6) = {y_interp:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(min(x), max(x), 100)\n",
    "y_vals = [lagrange_interpolation(xi, x, y) for xi in x_vals]\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Lagrange interpolating polynomial')\n",
    "plt.plot(x_interp, y_interp, 'go', label='Interpolated point (6)')\n",
    "plt.title(\"Lagrange's Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x)')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
