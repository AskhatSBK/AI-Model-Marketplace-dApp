   ],
   "source": [
    "import numpy as np\n",
    "from scipy.interpolate import CubicSpline\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([1, 2, 3, 4, 5])\n",
    "y = np.array([2.3, 3.1, 4.9, 6.5, 8.1])\n",
    "\n",
    "# Create cubic spline with natural boundary conditions\n",
    "cs = CubicSpline(x, y, bc_type='natural')\n",
    "\n",
    "# Evaluate at 2.5 and 4.3\n",
    "f_25 = cs(2.5)\n",
    "f_43 = cs(4.3)\n",
    "\n",
    "print(f\"Estimated f(2.5) = {f_25:.4f}\")\n",
    "print(f\"Estimated f(4.3) = {f_43:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(1, 5, 100)\n",
    "y_vals = cs(x_vals)\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Cubic Spline')\n",
    "plt.plot([2.5, 4.3], [f_25, f_43], 'go', label='Interpolated points')\n",
    "plt.title(\"Cubic Spline Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  }
