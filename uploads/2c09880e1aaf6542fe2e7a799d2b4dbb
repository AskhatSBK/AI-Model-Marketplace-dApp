   ],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([3, 4, 5, 6, 7])\n",
    "y = np.array([2.2, 3.5, 5.1, 7.3, 10.0])\n",
    "\n",
    "n = len(y)\n",
    "differences = [y.copy()]  # Initialize with the original y values\n",
    "\n",
    "# Correctly compute backward differences\n",
    "for i in range(1, n):\n",
    "    current_diff = []\n",
    "    prev_diff = differences[i-1]  # Previous differences\n",
    "    # Iterate j from the end of prev_diff down to 1\n",
    "    for j in range(len(prev_diff)-1, 0, -1):\n",
    "        current_diff.insert(0, prev_diff[j] - prev_diff[j-1])\n",
    "    differences.append(current_diff)\n",
    "\n",
    "# Function for Newton's Backward Interpolation\n",
    "def newton_backward(x_val, xn, h, diffs):\n",
    "    s = (x_val - xn) / h\n",
    "    result = diffs[0][-1]  # Start with the last y value\n",
    "    term = 1\n",
    "    for i in range(1, len(diffs)):\n",
    "        term *= (s + i - 1) / i\n",
    "        result += term * diffs[i][-1]  # Use the last difference at each level\n",
    "    return result\n",
    "\n",
    "h = x[1] - x[0]\n",
    "xn = x[-1]  # Last x value\n",
    "x_interp = 5.5\n",
    "y_interp = newton_backward(x_interp, xn, h, differences)\n",
    "\n",
    "print(f\"Estimated f(5.5) = {y_interp:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(min(x), max(x), 100)\n",
    "y_vals = [newton_backward(xi, xn, h, differences) for xi in x_vals]\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Interpolating polynomial')\n",
    "plt.plot(x_interp, y_interp, 'go', label='Interpolated point (5.5)')\n",
    "plt.title(\"Newton's Backward Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x)')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
