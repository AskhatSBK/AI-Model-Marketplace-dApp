   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Given data\n",
    "x = np.array([0, 1, 2, 3, 4])\n",
    "y = np.array([1, 2.7, 5.8, 10.4, 16.5])\n",
    "\n",
    "n = len(y)\n",
    "differences = [y.copy()]\n",
    "\n",
    "# Compute finite differences\n",
    "for i in range(1, n):\n",
    "    current_diff = []\n",
    "    for j in range(n - i):\n",
    "        current_diff.append(differences[i-1][j+1] - differences[i-1][j])\n",
    "    differences.append(current_diff)\n",
    "\n",
    "# Function for Newton's Forward Interpolation\n",
    "def newton_forward(x_val, x0, h, diffs):\n",
    "    s = (x_val - x0) / h\n",
    "    result = diffs[0][0]\n",
    "    term = s\n",
    "    result += term * diffs[1][0]\n",
    "    term *= (s - 1) / 2\n",
    "    result += term * diffs[2][0]\n",
    "    term *= (s - 2) / 3\n",
    "    result += term * diffs[3][0]\n",
    "    term *= (s - 3) / 4\n",
    "    result += term * diffs[4][0]\n",
    "    return result\n",
    "\n",
    "x0 = x[0]\n",
    "h = x[1] - x[0]\n",
    "x_interp = 2.5\n",
    "y_interp = newton_forward(x_interp, x0, h, differences)\n",
    "\n",
    "print(f\"Estimated f(2.5) = {y_interp:.4f}\")\n",
    "\n",
    "# Plotting\n",
    "x_vals = np.linspace(min(x), max(x), 100)\n",
    "y_vals = [newton_forward(xi, x0, h, differences) for xi in x_vals]\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(x, y, 'bo', label='Data points')\n",
    "plt.plot(x_vals, y_vals, 'r-', label='Interpolating polynomial')\n",
    "plt.plot(x_interp, y_interp, 'go', label='Interpolated point (2.5)')\n",
    "plt.title(\"Newton's Forward Interpolation\")\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('f(x)')\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()"
   ]
  },
